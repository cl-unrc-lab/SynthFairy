for i in {1..2}
do
	for j in {1..3}
	do
		let "a = $i * 60"
		let "b = $i * 8"
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.1 -q 0.1
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.5 -q 0.1
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.9 -q 0.1
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.1 -q 0.1
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.1 -q 0.5
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.5 -q 0.1
		python3 tests/scripts/robot_gen.py -s $j -l $a -w $b -p 0.5 -q 0.5
	done
done

for i in {1..2}
do
	for j in {1..3}
	do
		let "b = $i * 60"
		let "a = $i * 8"

		echo "Robot A $j-$a-$b-0.1"
		time ./bin/synthFairy   p=4 tests/synthesis/robotA[$j-$a-$b-0.1-0.1].sgg
		echo "Robot A $j-$a-$b-0.5"
		time ./bin/synthFairy   p=4 tests/synthesis/robotA[$j-$a-$b-0.5-0.5].sgg
		echo "Robot A $j-$a-$b-0.9"
		time ./bin/synthFairy   p=4 tests/synthesis/robotA[$j-$a-$b-0.9-0.1].sgg
	done
done

for i in {1..2}
do
	for j in {1..3}
	do
		let "b = $i * 60"
		let "a = $i * 8"

		echo "Robot B $j-$a-$b-0.1-0.1"
		time ./bin/synthFairy   p=4 tests/synthesis/robotB[$j-$a-$b-0.1-0.1].sgg
		echo "Robot B $j-$a-$b-0.1-0.5"
		time ./bin/synthFairy   p=4 tests/synthesis/robotB[$j-$a-$b-0.1-0.5].sgg
		echo "Robot B $j-$a-$b-0.5-0.1"
		time ./bin/synthFairy   p=4 tests/synthesis/robotB[$j-$a-$b-0.5-0.1].sgg
		echo "Robot B $j-$a-$b-0.5-0.5"
		time ./bin/synthFairy   p=4 tests/synthesis/robotB[$j-$a-$b-0.5-0.5].sgg
		
	done
done

for i in {1..2}
do
	for j in {1..3}
	do
		let "b = $i * 60"
		let "a = $i * 8"

		echo "Robot C $j-$a-$b-0.1-0.1"
		time ./bin/synthFairy   p=4 tests/synthesis/robotC[$j-$a-$b-0.1-0.1].sgg
		echo "Robot C $j-$a-$b-0.1-0.5"
		time ./bin/synthFairy   p=4 tests/synthesis/robotC[$j-$a-$b-0.1-0.5].sgg
		echo "Robot C $j-$a-$b-0.5-0.1"
		time ./bin/synthFairy   p=4 tests/synthesis/robotC[$j-$a-$b-0.5-0.1].sgg
		echo "Robot C $j-$a-$b-0.5-0.5"
		time ./bin/synthFairy   p=4 tests/synthesis/robotC[$j-$a-$b-0.5-0.5].sgg
		
	done
done